doctype html
<!--[if IE 9 ]
html(lang="en" class="ie9")
<![endif]-->
html
  head
    include head
  body
    div.page#page
      include header

      div.container
        div.ad
          div.ad--wide-shallow
            img(src="img/leader-0.gif" alt='leaderboard')

      div#main.content
        div.container
          section#primary.content__main
            article.story
              header.story__header
                figure.image.image--feature
                  picture
                    <!--[if IE 9]>
                    <video style="display: none">
                    <![endif]-->
                    source(srcset="img/caudle-omed.jpg" media="(min-width: 768px") 
                    source(srcset="img/caudle-omed@.50.jpg" media="(min-width: 480px") 
                    <!--[if IE 9]>
                    </video>
                    <![endif]-->
                    img(srcset="img/caudle-omed@.33.jpg" alt="Jennifer Caudle, DO")
                  //-img.image__img
                  figcaption.image__caption
              
                  
                h1.story__header--primary Media 101: How to position yourself as a medical expert

          section#secondary.content__secondary
            div.ad
              div.ad--med-rect
                img(src="img/medium-rect-0.gif" alt='medium-rectangle')
            div.ad
              div.ad--med-rect
                img(src="img/medium-rect-0.gif" alt='medium-rectangle')
      
    
    
    script(src='js/jquery.min.js')
    script(src='js/nav.js')
